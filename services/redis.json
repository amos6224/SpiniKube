{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "name": "spin-redis"
    },
    "spec": {
        "selector": {
            "app": "spin-redis"
        },
        "ports": [
            {
                "name": "redis",
                "protocol": "TCP",
                "port": 6379,
                "targetPort": 6379
            }
        ]
    }
}