{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "name": "spin-rosco"
    },
    "spec": {
        "selector": {
            "app": "spin-rosco"
        },
        "ports": [
            {
                "name": "rosco",
                "protocol": "TCP",
                "port": 8087,
                "targetPort": 8087
            }
        ]
    }
}