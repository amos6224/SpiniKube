{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "name": "spin-gate"
    },
    "spec": {
        "selector": {
            "app": "spin-gate"
        },
        "ports": [
            {
                "name": "gate",
                "protocol": "TCP",
                "port": 8084,
                "targetPort": 8084
            }
        ]
    }
}