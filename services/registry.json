{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "name": "spin-registry"
    },
    "spec": {
        "selector": {
            "app": "spin-registry"
        },
        "type": "NodePort",
        "ports": [
            {
                "name": "registry",
                "protocol": "TCP",
                "port": 5000,
                "targetPort": 5000
            },
            {
                "name": "ui",
                "protocol": "TCP",
                "port": 8080,
                "targetPort": 8080
            }
        ]
    }
}