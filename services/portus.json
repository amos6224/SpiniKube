{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "name": "spin-portus"
    },
    "spec": {
        "selector": {
            "app": "spin-portus"
        },
        "ports": [
            {
                "name": "portus",
                "protocol": "TCP",
                "port": 3000,
                "targetPort": 300
            }
        ]
    }
}