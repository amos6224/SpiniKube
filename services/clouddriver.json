{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "name": "spin-clouddriver"
    },
    "spec": {
        "selector": {
            "app": "spin-clouddriver"
        },
        "ports": [
            {
                "name": "clouddriver",
                "protocol": "TCP",
                "port": 7002,
                "targetPort": 7002
            }
        ]
    }
}