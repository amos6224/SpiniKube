apiVersion: v1
clusters:
- cluster:
    certificate-authority: /root/.kube/apiserver.crt
    server: https://192.168.99.100:443
  name: minikube
contexts:
- context:
    cluster: minikube
    user: minikube
  name: minikube
current-context: minikube
kind: Config
preferences: {}
users:
- name: minikube
  user:
    client-certificate: /root/.kube/apiserver.crt
    client-key: /root/.kube/apiserver.key
